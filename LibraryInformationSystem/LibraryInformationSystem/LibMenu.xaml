<Window x:Class="LibraryInformationSystem.LibMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryInformationSystem"
        mc:Ignorable="d"
        Title="LibMenu" Height="450" Width="1200">
    <Grid>
        <Frame x:Name="MainFrame" Margin="0,75,0,0"/>
        <Button Content="Публикации" HorizontalAlignment="Left" Margin="25,15,0,0" VerticalAlignment="Top" Height="30" Width="130" Click="Button_Click_Publication"/>
        <Button Content="Авторы" HorizontalAlignment="Left" Margin="180,15,0,0" VerticalAlignment="Top" Height="30" Width="130" Click="Button_Click_Authors"/>

    </Grid>
</Window>
